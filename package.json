{
  "name": "makesure",
  "description": "Process code changes with semantic rules",
  "version": "0.0.0",
  "author": "zetlen",
  "bin": {
    "makesure": "./bin/run.js"
  },
  "bugs": "https://github.com/zetlen/makesure/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "gitdiff-parser": "^0.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-json-schema-generator": "^2.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/zetlen/makesure",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "makesure",
    "dirname": "makesure",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "diff": {
        "description": "Analyze git diffs"
      }
    }
  },
  "repository": "zetlen/makesure",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "generate-schema": "ts-json-schema-generator --path lib/configuration/config.ts --type MakesureConfig > makesure-schema.json",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
